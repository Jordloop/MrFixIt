@{
    ViewData["Title"] = "Home";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h4>Welcome to Mr Fix-It</h4>

@if (User.Identity.IsAuthenticated)
{
    <p>@User.Identity.Name</p>
    <p>@Model.FirstName @Model.LastName</p>
    <div class="text-center">
        <h4>You are Logged In</h4>
    </div>
}


